{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 120,
  "proseWrap": "never",
  "endOfLine": "auto",
  "overrides": [{
      "files": ".prettierrc",
      "options": {
        "parser": "flow"
      }
    },
    {
      "files": ["*.html", "*.ejs"],
      "options": {
        "parser": "html"
      }
    }
  ]
}
